{
  "name": "edlab-messenger",
  "version": "1.1.9",
  "description": "",
  "main": "index.js",
  "scripts": {
    "debug": "node --inspect --trace-warnings --trace-deprecation server",
    "dev": "pm2 start ecosystem.config.js --only messenger-dev --no-daemon",
    "build": "npx babel server/ --copy-files -d dist",
    "start": "pm2-runtime start ecosystem.config.js --only app",
    "migrate": "sequelize --config config/DatabaseConfig.json db:migrate",
    "seed": "sequelize --config config/DatabaseConfig.json db:seed:all",
    "unseed": "sequelize --config config/DatabaseConfig.json db:seed:undo:all",
    "doc": "apidoc -v -i server/controllers -o docs/",
    "test": "NODE_ENV=test mocha \"tests/**/*.test.js\" --require babel-polyfill --require @babel/register --exit",
    "test:report": "NODE_ENV=test mocha \"tests/**/*.test.js\" --require babel-polyfill --require @babel/register --reporter mochawesome --reporter-options reportDir=tests/report,autoOpen  --exit",
    "test:coverage": "NODE_ENV=test nyc mocha \"tests/**/*.test.js\" --require babel-polyfill --require @babel/register --exit && nyc report --reporter=html && open coverage/index.html",
    "sendScheduledEmails": "NODE_ENV=production node server/scripts/sendScheduledEmails.js",
    "updateStatusLogs": "NODE_ENV=production node server/scripts/updateStatusLogs.js"
  },
  "engines": {
    "node": "10.x"
  },
  "author": "EdLab Dev Team <edlabit@tc.columbia.edu>",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-member-expression-literals": "^7.2.0",
    "@babel/plugin-transform-property-literals": "^7.2.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "aws-sdk": "^2.537.0",
    "axios": "^0.19.0",
    "babel-plugin-transform-merge-sibling-variables": "^6.9.4",
    "babel-plugin-transform-minify-booleans": "^6.9.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "cli-highlight": "^2.1.1",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv-safe": "^8.1.0",
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-get-param": "^1.1.2",
    "helmet": "^3.21.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "mysql2": "^1.7.0",
    "pm2": "^3.5.1",
    "sequelize": "^5.19.5",
    "string-template": "^1.0.0",
    "textversionjs": "^1.1.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "apidoc": "^0.17.7",
    "babel-eslint": "^10.0.3",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "mocha": "^5.2.0",
    "mochawesome": "^3.1.2",
    "nyc": "^13.3.0",
    "sequelize-cli": "^5.5.1",
    "supertest": "^3.4.2"
  }
}
