    "gulp": "^4.0.0",
    "mocha": "^5.2.0",
    "moment": "^2.23.0",
    "morgan": "^1.9.1",
    "pm2": "^3.2.8",
    "rimraf": "^2.6.3",
    "textversionjs": "^1.1.3"
  }
