{
  "name": "edlab-***REMOVED***",
  "version": "1.0.19",
  "description": "",
  "main": "index.js",
  "scripts": {
    "bump": "gulp bump",
    "bump:minor": "gulp bump-minor",
    "bump:major": "gulp bump-major",
    "dev": "pm2 start ecosystem.config.js --only ***REMOVED***-dev --no-daemon",
    "int": "pm2-docker start ecosystem.config.js --only ***REMOVED***-int",
    "prod": "pm2-docker start ecosystem.config.js --only ***REMOVED***-prod",
    "build": "npm run clean && babel server --copy-files -d dist",
    "clean": "rimraf public/*",
    "migrate": "sequelize --config config/DatabaseConfig.json db:migrate",
    "doc": "apidoc -v -i server/controllers -o docs/",
    "test": "NODE_ENV=test mocha \"tests/**/*.test.js\" --require babel-polyfill --require @babel/register --exit",
    "test:report": "NODE_ENV=test mocha \"tests/**/*.test.js\" --require babel-polyfill --require @babel/register --reporter mochawesome --reporter-options reportDir=test/report,autoOpen  --exit",
    "test:coverage": "NODE_ENV=test nyc mocha \"tests/**/*.test.js\" --require babel-polyfill --require @babel/register --exit && nyc report --reporter=html && open coverage/index.html"
  },
  "author": "EdLab Dev Team <edlabit@tc.columbia.edu>",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.401.0",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "cli-highlight": "^2.0.0",
    "cors": "^2.8.5",
    "cron": "^1.6.0",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-get-param": "^1.1.2",
    "helmet": "^3.15.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "pm2": "^3.2.9",
    "rimraf": "^2.6.3",
    "sequelize": "^4.42.0",
    "string-template": "^1.0.0",
    "textversionjs": "^1.1.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-transform-member-expression-literals": "^7.2.0",
    "@babel/plugin-transform-property-literals": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "apidoc": "^0.17.7",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-merge-sibling-variables": "^6.9.4",
    "babel-plugin-transform-minify-booleans": "^6.9.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "chai": "^4.2.0",
    "eslint": "^5.13.0",
    "gulp": "^4.0.0",
    "gulp-bump": "^3.1.3",
    "mocha": "^5.2.0",
    "mochawesome": "^3.1.1",
    "nyc": "^13.2.0",
    "sequelize-cli": "^5.4.0",
    "supertest": "^3.4.2"
  }
